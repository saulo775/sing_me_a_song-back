    "@types/jest": "^27.5.2",
    "@types/joi": "^17.2.3",
    "@types/node": "^16.3.3",
    "@types/supertest": "^2.0.11",
    "dotenv-cli": "^6.0.0",
    "eslint": "^7.32.0",
    "jest": "^28.1.3",
    "prisma": "^3.13.0",
    "supertest": "^6.1.3",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
